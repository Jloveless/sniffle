[{gproc, 
  [{gproc_dist, all}]},
 {sniffle, 
  []},
 {redgrid,
  [
   {url, "_REDIS_URL_"},
   {meta, []},
   {anonymous, false},
   {domain, "_REDIS_DOMAIN_"},
   {version, ""},
   {local_ip, "_OWN_IP_"}
  ]},
 {lager, 
  [{handlers, [
	       {lager_console_backend, info},
	       {lager_file_backend, [{"/var/log/fifo/sniffle/error.log", error},
				     {"/var/log/fifo/sniffle/warning.log", warning},
				     {"/var/log/fifo/sniffle/debug.log", debug},
				     {"/var/log/fifo/sniffle/console.log", info}
				    ]}
	      ]},
   {crash_log, "{{platform_log_dir}}/crash.log"},
	  {crash_log_size, 65536},
	  {error_logger_redirect, true}
  ]},
 {alog, 
  [{enabled_loggers, [alog_tty]},
   {flows, 
    [{{mod,['_']}, {'=<', warning}, [alog_tty]}]}]}
].

